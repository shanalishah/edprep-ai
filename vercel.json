{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://web-production-4d7f.up.railway.app",
    "NEXT_PUBLIC_AUTH_PROVIDER": "backend",
    "NEXT_PUBLIC_USE_SUPABASE_MENTORSHIP": "false"
  }
}
